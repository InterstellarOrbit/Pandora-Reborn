      <texPath>Things/Item/BodyPart/BodyPartBionic</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>1500</MarketValue>
      <Mass>1</Mass>
